{
  "name": "@abbos04/my-npm-package",
  "private": false,
  "version": "1.1.8",
  "description": "A small utility library (add, greet)",
  /* "type": "module",  <-- tavsiya: olib tashla yoki izohga ol. */
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --format cjs,esm --dts --out-dir dist",
    "prepare": "npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "test": "vitest"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.5.0",
    "typescript": "~5.9.3",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abbos772/My-npm-package-.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abbos772/My-npm-package-/issues"
  },
  "homepage": "https://github.com/abbos772/My-npm-package-#readme"
}
